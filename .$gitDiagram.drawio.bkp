        <mxCell id="_6Ugzg3BD1Cbug45hPQd-166" value="&lt;font style=&quot;font-size: 18px;&quot;&gt;&lt;b&gt;Git Commit&lt;/b&gt;&lt;/font&gt;" style="edgeLabel;html=1;align=center;verticalAlign=middle;resizable=0;points=[];" vertex="1" connectable="0" parent="_6Ugzg3BD1Cbug45hPQd-165">
